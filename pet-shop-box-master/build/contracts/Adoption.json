  "source": "pragma solidity ^0.5.0;\n\ncontract Adoption {\n  address[99] public adopters;\n  // Adopting a pet\n  function adopt(uint petId) public returns (uint) {\n    require(petId >= 0 && petId <= 99);\n\n    adopters[petId] = msg.sender;\n\n    return petId;\n  }\n  // Retrieving the adopters\n  function getAdopters() public view returns (address[99] memory) {\n    return adopters;\n  }\n\n}",
